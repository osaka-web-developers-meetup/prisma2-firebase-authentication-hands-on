{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["esnext","dom"],
    "sourceMap": true,
    "outDir": "./dist",
    "removeComments": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "moduleResolution": "node",
    "esModuleInterop": true
  },
  "exclude": [
    "node_modules",
    "prisma"
  ]
}
