datasource db {
  provider = "postgres"
  url      = "postgresql://postgres:password@db:5432/prisma_sample_development?schema=public"
}

generator photon {
  provider = "photonjs"
}

model Community {
  id          String    @default(cuid()) @id @unique
  name        String    @unique
  description String?
  createdAt   DateTime  @default(now())
  updatedAt   DateTime  @updatedAt
}
